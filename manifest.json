{
    "manifest_version": 3,
    "name": "New Tab Notes",
    "version": "1.0",
    "description": "Turn your New Tab into a note-taking tool.",
    "chrome_url_overrides": {
      "newtab": "index.html"
    },
    "permissions": ["storage"],
    "icons": {
      "16": "icon.jpg",
      "48": "icon.jpg",
      "128": "icon.jpg"
    }
  }
  